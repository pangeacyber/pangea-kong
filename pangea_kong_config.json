{
  "pangea_domain": "dev.aws.pangea.cloud",
  "ignore_pangea_domain": "host.docker.internal:8000",
  "ignore_insecure": true,
  "rules": [
    {
      "host": "api.openai.com",
      "endpoint": "/v1/chat/completions",
      "protocols": ["https"],
      "ports": [],
      "audit_values": {
        "model": ""
      },
      "format": "openai",
      "ai_guard": {
        "request": {
          "enabled": true,
          "config_id": "",
          "parameters": {
            "recipe": "",
            "overrides": {
              "prompt_injection": "",
              "ip_address_action": "",
              "url_address_action": "",
              "domain_address_action": ""
            }
          }
        },
        "response": {
        }
      }
    },
    {
      "host": "localhost",
      "endpoint": "/",
      "audit_values": {
        "model": ""
      },
      "format": "openai",
      "ai_guard": {
        "request": {
        }
      }
    }
  ]
}