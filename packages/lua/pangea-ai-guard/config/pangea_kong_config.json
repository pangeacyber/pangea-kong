{
  "pangea_domain": "aws.us.pangea.cloud",
  "insecure": false,
  "rules": [
    {
      "host": "localhost",
      "endpoint": "/v1/chat/completions",
      "prefix": "/chatgpt/dev",
      "allow_on_error": true,
      "parser": "openai",
      "ai_guard": {
        "request": {
          "parameters": {
            "recipe": "pangea_prompt_guard"
          }
        }
      }
    },
    {
      "host": "localhost",
      "endpoint": "/v1/chat/completions",
      "prefix": "/chatgpt/user",
      "allow_on_error": false,
      "parser": "openai",
      "ai_guard": {
        "request": {
          "parameters": {
            "recipe": "pangea_prompt_guard"
          }
        }
      }
    },
    {
      "host": "localhost",
      "endpoint": "/v2/chat",
      "prefix": "/cohere/dev",
      "allow_on_error": true,
      "parser": "cohere",
      "ai_guard": {
        "request": {
          "parameters": {
            "recipe": "pangea_prompt_guard"
          }
        }
      }
    },
    {
      "host": "localhost",
      "endpoint": "/v2/chat",
      "prefix": "/cohere/user",
      "allow_on_error": false,
      "parser": "cohere",
      "ai_guard": {
        "request": {
          "parameters": {
            "recipe": "pangea_prompt_guard"
          }
        }
      }
    },
    {
      "host": "localhost",
      "endpoint": "/v1/messages",
      "prefix": "/claude/dev",
      "allow_on_error": true,
      "parser": "claude",
      "ai_guard": {
        "request": {
          "parameters": {
            "recipe": "pangea_prompt_guard"
          }
        }
      }
    },
    {
      "host": "localhost",
      "endpoint": "/v1/messages",
      "prefix": "/claude/user",
      "allow_on_error": false,
      "parser": "claude",
      "ai_guard": {
        "request": {
          "parameters": {
            "recipe": "pangea_prompt_guard"
          }
        }
      }
    }
  ]
}